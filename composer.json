{
    "name": "nepada/message-bus-doctrine",
    "type": "library",
    "license": "BSD-3-Clause",
    "description": "Doctrine ORM integration for nepada/message-bus",
    "keywords": ["message bus", "doctrine"],
    "support": {
        "email": "petr@pada.cz",
        "issues": "https://github.com/nepada/message-bus-doctrine/issues"
    },
    "authors": [
        {
            "name": "Petr MorÃ¡vek",
            "email": "petr@pada.cz"
        }
    ],
    "require": {
        "php": ">=7.4.0",
        "doctrine/orm": "^2.7@dev",
        "doctrine/persistence": "^1.3.0 || ^2.0",
        "nepada/message-bus": "^2.0@dev"
    },
    "require-dev": {
        "nette/tester": "2.4.0",
        "phpstan/phpstan": "0.12.90",
        "phpstan/phpstan-strict-rules": "0.12.9",
        "pepakriz/phpstan-exception-rules": "0.11.7",
        "nepada/phpstan-nette-tester": "0.3.1",
        "spaze/phpstan-disallowed-calls": "1.4.0",
        "php-parallel-lint/php-parallel-lint": "1.3.0",
        "nepada/coding-standard": "7.4.0"
    },
    "autoload": {
        "psr-4": {
            "Nepada\\MessageBusDoctrine\\": "src/MessageBusDoctrine/"
        }
    },
    "autoload-dev": {
        "psr-4": {
            "NepadaTests\\": "tests/"
        }
    },
    "config": {
        "platform-check": false
    },
    "extra": {
        "branch-alias": {
            "dev-master": "2.0-dev"
        }
    }
}
