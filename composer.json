{
    "name": "nepada/message-bus-doctrine",
    "type": "library",
    "license": "BSD-3-Clause",
    "description": "Doctrine ORM integration for nepada/message-bus",
    "keywords": ["message bus", "doctrine"],
    "support": {
        "email": "petr@pada.cz",
        "issues": "https://github.com/nepada/message-bus-doctrine/issues"
    },
    "authors": [
        {
            "name": "Petr MorÃ¡vek",
            "email": "petr@pada.cz"
        }
    ],
    "require": {
        "php": ">=8.1.0 <8.4",
        "doctrine/orm": "^2.13@dev || ^3.0@dev",
        "doctrine/persistence": "^2.5 || ^3.0",
        "nepada/message-bus": "^2.0@dev || ^3.0@dev"
    },
    "require-dev": {
        "nette/tester": "2.5.3",
        "phpstan/phpstan": "1.11.3",
        "phpstan/phpstan-strict-rules": "1.6.0",
        "nepada/phpstan-nette-tester": "1.2.0",
        "spaze/phpstan-disallowed-calls": "3.4.0",
        "shipmonk/phpstan-rules": "3.0.0",
        "php-parallel-lint/php-parallel-lint": "1.4.0",
        "nepada/coding-standard": "7.14.0",
        "doctrine/dbal": ">=2.13.3",
        "symfony/cache": ">=6.0",
        "composer-runtime-api": "^2.0",
        "composer/semver": "3.4.0"
    },
    "autoload": {
        "psr-4": {
            "Nepada\\MessageBusDoctrine\\": "src/MessageBusDoctrine/"
        }
    },
    "autoload-dev": {
        "psr-4": {
            "NepadaTests\\": "tests/"
        }
    },
    "config": {
        "platform-check": false,
        "allow-plugins": {
            "dealerdirect/phpcodesniffer-composer-installer": false
        }
    },
    "extra": {
        "branch-alias": {
            "dev-master": "2.3-dev"
        }
    }
}
