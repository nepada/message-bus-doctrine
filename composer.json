{
    "name": "nepada/message-bus-doctrine",
    "type": "library",
    "license": "BSD-3-Clause",
    "description": "Doctrine ORM integration for nepada/message-bus",
    "keywords": ["message bus", "doctrine"],
    "support": {
        "email": "petr@pada.cz",
        "issues": "https://github.com/nepada/message-bus-doctrine/issues"
    },
    "authors": [
        {
            "name": "Petr MorÃ¡vek",
            "email": "petr@pada.cz"
        }
    ],
    "require": {
        "php": ">=7.4.0 <8.3",
        "doctrine/orm": "^2.7@dev",
        "doctrine/persistence": "^1.3.0 || ^2.0 || ^3.0",
        "nepada/message-bus": "^2.0@dev"
    },
    "require-dev": {
        "nette/tester": "2.4.3",
        "phpstan/phpstan": "1.8.11",
        "phpstan/phpstan-strict-rules": "1.4.4",
        "nepada/phpstan-nette-tester": "1.0.0",
        "spaze/phpstan-disallowed-calls": "2.5.0",
        "shipmonk/phpstan-rules": "1.2.0",
        "php-parallel-lint/php-parallel-lint": "1.3.2",
        "nepada/coding-standard": "7.5.2",
        "doctrine/cache": "^1.11",
        "doctrine/dbal": ">=2.13.3",
        "doctrine/annotations": ">=1.8"
    },
    "autoload": {
        "psr-4": {
            "Nepada\\MessageBusDoctrine\\": "src/MessageBusDoctrine/"
        }
    },
    "autoload-dev": {
        "psr-4": {
            "NepadaTests\\": "tests/"
        }
    },
    "config": {
        "platform-check": false,
        "allow-plugins": {
            "dealerdirect/phpcodesniffer-composer-installer": false
        }
    },
    "extra": {
        "branch-alias": {
            "dev-master": "2.1-dev"
        }
    }
}
