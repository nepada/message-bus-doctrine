includes:
    - vendor/phpstan/phpstan-strict-rules/rules.neon
    - vendor/nepada/phpstan-nette-tester/extension.neon
    - tests/PHPStan/disallowedCalls.neon

parameters:
    level: max
    paths:
        - tests

    fileExtensions:
        - php
        - phpt

    excludes_analyse:
        - tests/temp/*
